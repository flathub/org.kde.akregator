    "base-version": "5.15-21.08",
    "command": "akregator",
    "rename-icon": "akregator",
    "finish-args": [
        "--socket=fallback-x11",
        "--socket=wayland",
        "--share=network",
        "--share=ipc",
        "--device=dri",
