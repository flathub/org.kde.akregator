    "base-version": "5.15-22.08",
    "command": "akregator",
    "rename-icon": "akregator",
    "finish-args": [
        "--socket=fallback-x11",
        "--socket=wayland",
        "--share=network",
        "--share=ipc",
        "--device=dri",
        "--own-name=org.freedesktop.Akonadi",
        "--own-name=org.freedesktop.Akonadi.*",
